  <form id="login--form" class="form_container">
    <!--Error msg-->
    <% if (req.session.userExist) { %>
      <div class="error--msg">
        <div><img class="warning--icon" src="images/warning.png"></div> 
        <div class="content">
          <p>Your account is already registered,</p>
          <p>please try to login to your account</p>
        </div>
      </div>
      <% delete req.session.userExist %>
    <% } %>
    

    <div class="login--error--msg">
      <div><img class="warning--icon" src="images/warning.png"></div> 
      <div class="content">
        <p class="errorMsg"></p>
      </div>
    </div>

