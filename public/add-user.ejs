    <div class="container">
      <h1 class="h1">Create Employee Account</h1>
      <form id="add-user-form">
        <div class="center-input">
          <label for="username" class="custom-label">First name:</label>
          <input
            type="text"
            id="firstname"
            name="username"
            class="custom-input"
            required
          />

          <label for="username" class="custom-label">Last name:</label>
          <input
            type="text"
            id="username"
            name="lastname"
            class="custom-input"
            required
          />

          <label for="username" class="custom-label">Email:</label>
          <input
            type="text"
            id="username"
            name="email"
            class="custom-input"
            required
          />

          <label for="phone" class="custom-label">Phone Number:</label>
          <input
            type="text"
            id="phone"
            name="phonenumber"
            class="custom-input"
            required
          />

          <label for="address" class="custom-label">Address:</label>
          <textarea
            id="address"
            name="address"
            rows="4"
            required
            class="custom-input"
          ></textarea>

          <label for="position" class="custom-label">Position:</label>
          <select id="position" name="position" class="custom-select">
            <option value="Assistant Manager">Assistant Manager</option>
            <option value="Branch 2">Branch 2</option>
            <option value="Branch 3">Branch 3</option>
          </select>

          <label for="branch" class="custom-label">Branch:</label>
          <select id="branch" name="branch" class="custom-select">
            <option value="Branch 1">Branch 1</option>
            <option value="Branch 2">Branch 2</option>
            <option value="Branch 3">Branch 3</option>
          </select>

          <!-- Password field -->
          <label for="password" class="custom-label">Password:</label>
          <div class="password-container">
            <input
              type="password"
              id="password"
              name="password"
              class="custom-input"
              required
            />
            <span class="password-toggle" id="password_toggle">ðŸ™ˆ</span>
          </div>

          <!-- Confirm Password field -->
          <label for="confirm_password" class="custom-label"
            >Confirm Password:</label
          >
          <div class="password-container">
            <input
              type="password"
              id="confirm_password"
              name="confirm_password"
              class="custom-input"
              required
            />
            <span class="password-toggle" id="confirm_password_toggle">ðŸ™ˆ</span>
          </div>

          <!-- Password strength estimation -->
          <div class="custom-password-strength">
            <span>Password Strength:</span>
            <progress
              id="password_strength_bar"
              value="0"
              max="4"
              class="custom-password-strength-bar"
            ></progress>
            <span id="password_strength_text">Weak</span>
          </div>

          <!-- Weak password alert message -->
          <div class="custom-password-alert"></div>

          <!-- Error message -->
          <div class="custom-error-message"></div>

          <!-- Success message -->
          <div class="custom-success-message"></div>
        </div>

        <button type="submit" class="custom-button">
          <div>Create Account</div>
          <div><img class="progress" src="../../images/progress2.svg" /></div>
        </button>
      </form>
    </div>

